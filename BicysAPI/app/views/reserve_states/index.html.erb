<p id="notice"><%= notice %></p>

<h1>Reserve States</h1>

<table>
  <thead>
    <tr>
      <th>Reserve state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reserve_states.each do |reserve_state| %>
      <tr>
        <td><%= reserve_state.reserve_state %></td>
        <td><%= link_to 'Show', reserve_state %></td>
        <td><%= link_to 'Edit', edit_reserve_state_path(reserve_state) %></td>
        <td><%= link_to 'Destroy', reserve_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserve State', new_reserve_state_path %>
<%= link_to 'Index', root_path %>