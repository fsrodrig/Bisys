<p id="notice"><%= notice %></p>

<h1>Reserve Histories</h1>

<table>
  <thead>
    <tr>
      <th>Initial date</th>
      <th>Final date</th>
      <th>Reserve state</th>
      <th>Id reserve</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reserve_histories.each do |reserve_history| %>
      <tr>
        <td><%= reserve_history.initial_date %></td>
        <td><%= reserve_history.final_date %></td>
        <td><%= reserve_history.reserve_state %></td>
        <td><%= reserve_history.id_reserve %></td>
        <td><%= link_to 'Show', reserve_history %></td>
        <td><%= link_to 'Edit', edit_reserve_history_path(reserve_history) %></td>
        <td><%= link_to 'Destroy', reserve_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserve History', new_reserve_history_path %>
